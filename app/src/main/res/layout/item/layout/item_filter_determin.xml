<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="120dp">

    <com.eki.parking.View.libs.StateButton
        android:id="@+id/determinBtn"
        android:layout_width="260dp"
        android:layout_height="50dp"

        android:text="@string/Determine"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.65"
        app:normalBackgroundColor="@color/Eki_orange_4"
        app:normalTextColor="@color/color_white"
        app:pressedBackgroundColor="@color/Eki_orange_1"
        app:pressedTextColor="@color/color_white"
        app:unableBackgroundColor="@color/light_gray6"
        app:unableTextColor="@color/light_gray5"
        app:round="true" />

</androidx.constraintlayout.widget.ConstraintLayout>