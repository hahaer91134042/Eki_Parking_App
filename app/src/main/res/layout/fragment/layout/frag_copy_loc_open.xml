<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.eki.parking.View.recycleview.BaseRecycleView
        android:id="@+id/recycleView"
        android:layout_width="match_parent"
        android:layout_height="0dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/determinBtn"
        />


    <com.eki.parking.View.libs.StateButton
        android:id="@+id/determinBtn"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:enabled="false"
        android:gravity="center"
        android:text="@string/Save"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        app:normalBackgroundColor="@color/Eki_green_2"
        app:normalTextColor="@color/color_white"
        app:pressedBackgroundColor="@color/Eki_green_3"
        app:pressedTextColor="@color/color_white"
        app:unableBackgroundColor="@color/Eki_green_3"
        app:unableTextColor="@color/color_white" />
</androidx.constraintlayout.widget.ConstraintLayout>